<local:BoolSettingsNodeViewBase
    x:Class="Mutagen.Bethesda.WPF.Reflection.Fields.BoolSettingsNodeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Mutagen.Bethesda.WPF.Reflection.Fields"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <local:BoolSettingsNodeViewBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Mutagen.Bethesda.WPF;component/Reflection/Fields/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </local:BoolSettingsNodeViewBase.Resources>
    <Border Style="{StaticResource ReflectionSettingBorder}">
        <StackPanel Orientation="Horizontal">
            <TextBlock
                x:Name="SettingNameBlock"
                Margin="4,0,6,0"
                VerticalAlignment="Center"
                Style="{StaticResource ReflectionSettingName}" />
            <CheckBox x:Name="Checkbox" />
        </StackPanel>
    </Border>
</local:BoolSettingsNodeViewBase>
